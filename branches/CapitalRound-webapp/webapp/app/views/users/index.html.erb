<%= render 'teaser' %>
<%= render 'usernav' %>
<% if flash[:success] %>
    <p class="notice" style="color:#46a546;"><%= flash[:success] %></p>

<% end %>
<p id="notice"><%= @shortnotice %></p>

<h3>News feed</h3>
<% if @user_updates != nil %>
<%= render :partial => 'companyupdates/news', :collection => @user_updates %>
<% else %>
    <% if session[:owner_type]==1 %>
<p>You do not have news to display</p>
    <% else %>
<p>This person does not have news to display</p>
    <% end %>
<% end %>
