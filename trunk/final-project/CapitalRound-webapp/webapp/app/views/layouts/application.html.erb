      <nav>
              <ul class="nav pull-left">
            <li><h4 style="padding-left:20px;"><%= link_to 'Capital Round', :controller => 'home'  %></h4>
            
            </li>
            </ul>
          </nav>
              <ul class="nav pull-right">
         <% if session[:id] == nil || session[:id] == 0 %>
                <%= link_to 'Login', :controller => 'authentications', :action => 'login' %> |
                <%= link_to 'Join Us', authentications_new_path %>
          <% else %>
            <% user = User.find(session[:id])  %>
            <%= link_to :controller => 'users', :id => session[:id] do %>
                <%= user.firstname %> <%= user.lastname %>
            <% end %> |

            <% if user.Owners.length != 0 %>
                <%= link_to :controller => 'startups', :action => "show", :id => user.Owner_startups.first.id do %>
                    <%= user.Owner_startups.first.name %>
                <% end %> |
            <% end %>
          <%= link_to 'Logout', :controller => 'authentications', :action => 'logout' %> 
        <% end %>
      </ul>

      </div>
    
      
</div>
